from transformers import AutoProcessor, CLIPModel
from sdpipeline import PILTools as pt
import torch
import torch.nn.functional as F
import mlops_utils
import os
from imp import reload
import torchvision
import mlops_image_utils as imutils
import numpy

device = node.parent().parm(\"device\").evalAsString()
model = node.parent().parm(\"model\").evalAsString()
cache_only = node.parent().parm(\"cache_only\").evalAsInt()

model_path = mlops_utils.ensure_huggingface_model_local(model, os.path.join(\"$MLOPS\", \"data\", \"models\", \"transformers\"), bool(cache_only), model_type=\"transformers\")
model = CLIPModel.from_pretrained(model_path)

processor = AutoProcessor.from_pretrained(model_path)

geo1 = hou.pwd().inputGeometry(1)
geo2 = hou.pwd().inputGeometry(2)

# Attributes
width = int(geo1.attribValue(\"image_dimension\")[0])
height = int(geo1.attribValue(\"image_dimension\")[1])
r = numpy.array(geo1.pointFloatAttribValues(\"r\"), dtype=numpy.float32).reshape(width, height)
g = numpy.array(geo1.pointFloatAttribValues(\"g\"), dtype=numpy.float32).reshape(width, height)
b = numpy.array(geo1.pointFloatAttribValues(\"b\"), dtype=numpy.float32).reshape(width, height)
input_colors = numpy.stack((r,g,b), axis=0)
image1 = imutils.colors_numpy_array_to_pil(input_colors)

# Attributes
width = int(geo2.attribValue(\"image_dimension\")[0])
height = int(geo2.attribValue(\"image_dimension\")[1])
r = numpy.array(geo2.pointFloatAttribValues(\"r\"), dtype=numpy.float32).reshape(width, height)
g = numpy.array(geo2.pointFloatAttribValues(\"g\"), dtype=numpy.float32).reshape(width, height)
b = numpy.array(geo2.pointFloatAttribValues(\"b\"), dtype=numpy.float32).reshape(width, height)
input_colors = numpy.stack((r,g,b), axis=0)
image2 = imutils.colors_numpy_array_to_pil(input_colors)

image1, image2 = imutils.ensure_same_pil_image_dimensions(image1, image2)

transform = torchvision.transforms.Compose([torchvision.transforms.ToTensor()])
transform(image1).to(device)
transform(image2).to(device)

inputs1 = processor(images=image1, return_tensors=\"pt\")
image_features1 = model.get_image_features(**inputs1)

inputs2 = processor(images=image2, return_tensors=\"pt\")
image_features2 = model.get_image_features(**inputs2)


# Reshape the tensors to be 2-D
tensor1_2d = image_features1.view(2, -1)
