--HOUDINIMIMEBOUNDARY0xD3ADD339-0x00000F49-0x56B122C9-0x00000001HOUDINIMIMEBOUNDARY
Content-Disposition: attachment; filename="hdaroot/python2.parm"
Content-Type: text/plain

{
version 0.8
python	[ 0	locks=0 ]	(	"import numpy as np
import umap

node = hou.pwd()
geo = node.geometry()
geo1 = hou.pwd().inputGeometry(1)
geo2 = hou.pwd().inputGeometry(2)

clusterdims = geo1.attribValue(\"clusterdim\")

#---UMAP---
def draw_umap(data, n_neighbors=4, min_dist=.01, n_components=clusterdims, metric='euclidean', title=''):
    fit = umap.UMAP(
        n_neighbors=n_neighbors,
        min_dist=min_dist,
        n_components=n_components,
        metric=metric,
        random_state=42
    )
    u = fit.fit_transform(data)
    return u

    
#---Main---
num_latent_elements = geo1.attribValue(\"data_size\")

vals = geo1.floatListAttribValue(\"input_data\")
subarrays = [vals[i:i+num_latent_elements] for i in range(0, len(vals), num_latent_elements)]
flattened_data = subarrays

dimred = draw_umap(flattened_data)
out = dimred.tolist()

n = 0
geo.addAttrib(hou.attribType.Point, \"class\", -1, create_local_variable=False)
for subarray in out:
    point = geo.createPoint()
    if(clusterdims == 3):
        point.setAttribValue(\"P\", [subarray[0], subarray[1], subarray[2]])
    elif(clusterdims == 2):
        point.setAttribValue(\"P\", [subarray[0], subarray[1], 0.0])
    elif(clusterdims == 1):
        point.setAttribValue(\"P\", [subarray[0], 0.0, 0.0])
        
    point.setAttribValue(\"class\", n)
    n += 1"	)
maintainstate	[ 0	locks=0 ]	(	"off"	)
}

